<% if @feed_items.any? %>
  <ol class="microposts">
    <%= render @feed_items %>
  </ol>
<!--  Paginate feed item in static page controller home action. If not, paginate trying to get from microposts?page=:page-->
  <%= will_paginate @feed_items, params: {controller: :static_pages, action: :home} %>
<% end %>